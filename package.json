{
  "name": "my-home-page",
  "version": "0.0.0",
  "description": "My Home Page",
  "engines": {
    "node": "4.4.4"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "browserify": "^13.3.0",
    "chai": "^3.5.0",
    "httpbackend": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^2.5.3",
    "ng-html2js": "^3.0.0",
    "nodemailer-mock-transport": "^1.3.0",
    "nodemon": "^1.11.0",
    "npm-watch": "^0.1.7",
    "phantomjs": "^2.1.7",
    "protractor": "^5.0.0",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2"
  },
  "dependencies": {
    "angular": "^1.5.5",
    "angular-scroll": "^1.0.0",
    "angular-ui-router": "^0.3.0",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "express": "^4.13.4",
    "jquery": "^2.2.3",
    "ngalertify": "^1.0.2",
    "nodemailer": "^2.4.2",
    "path": "^0.12.7"
  },
  "scripts": {
    "postinstall": "./scripts/build/run.sh",
    "build": "./scripts/build/run.sh",
    "functionalTest": "./scripts/functionalTest/run.sh",
    "clientUnitTest": "karma start client/test/conf.js --single-run",
    "start": "node server/src/app.js",
    "startDev": "nodemon --watch server server/src/app.js",
